{
    "default": {
        "dryRun": false,
        "formatOptions": {
            "snippetInterface": "async-await"
        },
        "paths": [
            "src/test/features/**/*.feature"
        ],
        "require": [
            "src/test/steps/**/*.ts"
        ],
        "requireModule": [
            "ts-node/register"
        ],
        "format": [
            ["json","reports/cucumber_report.json"],
            ["html","reports/html-formate.html"],
            "progress"
        ]
    }
}